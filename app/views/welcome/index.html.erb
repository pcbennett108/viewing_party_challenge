<%= button_to "Create New User", register_path, method: :get %>

<div>
    <% if inactive_session? %>
        <h3>Welcome!</h3>
        <p>You must Log In or Register to see your dashboard.</p>
    <% elsif current_user %>
        <%= button_to "My Dashboard", user_path(current_user.id), method: :get %>
        <section class='existing-users'>
            <p>Existing Users: </p>
            <% @users.each do |user| %>
                <li><%= link_to user.email, user_path(user.id) %></li>
            <% end %>
        </section>
    <% end %>
</div>